{
  "name": "boilerplate_apis",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && pm2 start build/server.js --name 'boilerplate_apis' -dev",
    "restart": "npm run build && pm2 restart boilerplate_apis",
    "predev": " npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "export AWS_PROFILE=digismart && export AWS_SDK_LOAD_CONFIG=1 && concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec",
    "prestart": "mkdir -p build/template && cp -a src/template build/",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/server.ts"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/moment": "^2.13.0",
    "aws-cli": "^0.0.2",
    "aws-sdk": "^2.1577.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "bull": "^4.12.2",
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.9",
    "express": "^4.17.2",
    "express-rate-limit": "^7.2.0",
    "firebase": "^10.9.0",
    "firebase-admin": "^12.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "joi-objectid": "^4.0.2",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.35",
    "moment": "^2.30.1",
    "mongoose": "^8.2.1",
    "mongoose-lean-virtuals": "^0.9.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "multer-s3": "^3.0.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.7.5",
    "sharp": "^0.33.3",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.1.4",
    "twilio": "^5.0.1",
    "winston": "^3.12.0",
    "xlsx": "^0.18.5"
  },
  "author": "STS",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/busboy": "^1.5.3",
    "@types/cors": "^2.8.12",
    "@types/ejs": "^3.1.5",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/hapi__joi": "^17.1.8",
    "@types/json2csv": "^5.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mime-types": "^2.1.4",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.4",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.57.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.9.2"
  }
}
