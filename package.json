{
  "name": "boilerplate_apis",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && pm2 start build/server.js --name 'boilerplate_apis' -dev",
    "restart": "npm run build && pm2 restart boilerplate_apis",
    "predev": " npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "export AWS_PROFILE=digismart && export AWS_SDK_LOAD_CONFIG=1 && concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec",
    "prestart": "mkdir -p build/template && cp -a src/template build/",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/server.ts"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.4",
    "@types/moment": "^2.13.0",
    "aws-cli": "^0.0.2",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "bull": "^4.16.5",
    "busboy": "^1.6.0",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "firebase": "^11.1.0",
    "firebase-admin": "^13.0.2",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "joi-objectid": "^4.0.2",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "moment": "^2.30.1",
    "mongoose": "^8.2.1",
    "mongoose-lean-virtuals": "^1.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "music-metadata": "^10.6.4",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.16",
    "sharp": "^0.33.5",
    "stripe": "^17.5.0",
    "swagger-ui-express": "^5.0.1",
    "translatte": "^3.0.1",
    "tsoa": "^6.6.0",
    "twilio": "^5.4.0",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5"
  },
  "author": "STS",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/busboy": "^1.5.4",
    "@types/cors": "^2.8.12",
    "@types/ejs": "^3.1.5",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/json2csv": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mime-types": "^2.1.4",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.12",
    "@types/nodemailer": "^6.4.17",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.17.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2"
  }
}
